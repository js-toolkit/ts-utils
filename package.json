{
  "name": "@vzh/ts-types",
  "version": "0.7.0",
  "description": "Typescript helper types",
  "author": "Vladimir Zhukov",
  "license": "MIT",
  "contributors": [
    {
      "name": "Vladimir Zhukov",
      "url": "http://github.com/vlazh"
    }
  ],
  "homepage": "https://github.com/vlazh/vzh-ts-types",
  "repository": {
    "type": "git",
    "url": "https://github.com/vlazh/vzh-ts-types"
  },
  "bugs": {
    "url": "https://github.com/vlazh/vzh-ts-types/issues"
  },
  "main": "./index.js",
  "types": "./index.d.ts",
  "scripts": {
    "clean": "rimraf dist/",
    "copy": "copyfiles package.json LICENSE README.md ./dist/",
    "build": "npm run clean && tsc --project ./tsconfig.json && npm run copy",
    "build-to-tutoron": "npm run clean && tsc --project ./tsconfig.json --outDir '/home/vladimir/dev/sources/tutoron/node_modules/@vzh/ts-types/'",
    "build-to-pwa": "npm run clean && tsc --project ./tsconfig.json --outDir '/home/vladimir/dev/sources/pwa/node_modules/@vzh/ts-types/'",
    "build-to-reflexy": "npm run clean && tsc --project ./tsconfig.json --outDir '/home/vladimir/dev/sources/reflexy/node_modules/@vzh/ts-types/'",
    "build-to-react-components": "npm run clean && tsc --project ./tsconfig.json --outDir '../react-components/node_modules/@vzh/ts-types/'",
    "build-to-mobx-stores": "npm run clean && tsc --project ./tsconfig.json --outDir '../mobx-stores/node_modules/@vzh/ts-types/'",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "patch-publish": "npm version patch && npm run build && npm publish ./dist --access public && git push --follow-tags",
    "minor-publish": "npm version minor && npm run build && npm publish ./dist --access public && git push --follow-tags",
    "major-publish": "npm version major && npm run build && npm publish ./dist --access public && git push --follow-tags"
  },
  "devDependencies": {
    "@types/jest": "^24.0.15",
    "@typescript-eslint/eslint-plugin": "^1.11.0",
    "@typescript-eslint/parser": "^1.11.0",
    "@vzh/configs": "../configs/dist/",
    "babel-eslint": "^10.0.2",
    "copyfiles": "^2.1.0",
    "eslint": "^6.0.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-prettier": "^3.1.0",
    "jest": "^24.8.0",
    "prettier": "^1.18.2",
    "rimraf": "^2.6.2",
    "ts-jest": "^24.0.2",
    "typescript": "^3.5.2"
  }
}
