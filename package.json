{
  "name": "@vzh/ts-types",
  "version": "0.4.2",
  "description": "Typescript helper types",
  "author": "Vladimir Zhukov",
  "license": "MIT",
  "contributors": [
    {
      "name": "Vladimir Zhukov",
      "url": "http://github.com/vlazh"
    }
  ],
  "homepage": "https://github.com/vlazh/vzh-ts-types",
  "repository": {
    "type": "git",
    "url": "https://github.com/vlazh/vzh-ts-types"
  },
  "bugs": {
    "url": "https://github.com/vlazh/vzh-ts-types/issues"
  },
  "main": "./",
  "types": "./index.d.ts",
  "scripts": {
    "clean": "rimraf dist/",
    "copy": "copyfiles package.json LICENSE README.md ./dist/",
    "build": "npm run clean && tsc --project ./tsconfig.json && npm run copy",
    "build-to-tutoron": "npm run clean && tsc --project ./tsconfig.json --outDir '../tutoron/node_modules/@vzh/ts-types/'",
    "build-to-reflexy": "npm run clean && tsc --project ./tsconfig.json --outDir '../reflexy/node_modules/@vzh/ts-types/'",
    "build-to-react-components": "npm run clean && tsc --project ./tsconfig.json --outDir '../@vzh-react-components/node_modules/@vzh/ts-types/'",
    "build-to-mobx-stores": "npm run clean && tsc --project ./tsconfig.json --outDir '../@vzh-mobx-stores/node_modules/@vzh/ts-types/'",
    "patch-publish": "npm version patch && npm run build && npm publish ./dist --access public && git push --follow-tags",
    "minor-publish": "npm version minor && npm run build && npm publish ./dist --access public && git push --follow-tags",
    "major-publish": "npm version major && npm run build && npm publish ./dist --access public && git push --follow-tags"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^1.2.0",
    "@typescript-eslint/parser": "^1.2.0",
    "@vzh/configs": "^3.15.0",
    "babel-eslint": "^10.0.1",
    "copyfiles": "^2.1.0",
    "eslint": "^5.13.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-prettier": "^3.0.1",
    "prettier": "^1.16.4",
    "rimraf": "^2.6.2",
    "typescript": "^3.3.3",
    "webpack-merge": "^4.2.1"
  }
}
