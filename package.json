{
  "name": "@vzh/ts-types",
  "version": "1.8.1",
  "description": "Typescript helper types",
  "author": "Vladimir Zhukov",
  "license": "MIT",
  "main": "./index.js",
  "types": "./index.d.ts",
  "scripts": {
    "clean": "rimraf dist/*",
    "copy:configs": "copyfiles .npmignore package.json LICENSE README.md ./dist/",
    "build": "npm run clean && tsc --project ./tsconfig.json",
    "build-to-tutoron": "tsc --project ./tsconfig.json --outDir '/home/vladimir/dev/sources/tutoron/node_modules/@vzh/ts-types/'",
    "build-to-reflexy": "tsc --project ./tsconfig.json --outDir '/home/vladimir/dev/sources/reflexy/node_modules/@vzh/ts-types/'",
    "build-to-react-components": "tsc --project ./tsconfig.json --outDir '../react-components/node_modules/@vzh/ts-types/'",
    "build-to-mobx-utils": "tsc --project ./tsconfig.json --outDir '../mobx-utils/node_modules/@vzh/ts-types/'",
    "build-to-player": "tsc --project ./tsconfig.json --outDir '/home/vladimir/dev/sources/player/node_modules/@vzh/ts-types'",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "patch-publish": "npm run build && npm version patch && npm run copy:configs && npm publish ./dist --access public && git push --follow-tags",
    "minor-publish": "npm run build && npm version minor && npm run copy:configs && npm publish ./dist --access public && git push --follow-tags",
    "major-publish": "npm run build && npm version major && npm run copy:configs && npm publish ./dist --access public && git push --follow-tags"
  },
  "devDependencies": {
    "@types/jest": "^26.0.4",
    "@typescript-eslint/eslint-plugin": "^3.6.0",
    "@typescript-eslint/parser": "^3.6.0",
    "@vzh/configs": "^3.65.0",
    "copyfiles": "^2.3.0",
    "eslint": "^7.4.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.1.0",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.1.2",
    "typescript": "^3.9.6"
  }
}
