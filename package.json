{
  "name": "@vzh/ts-types",
  "version": "0.8.0",
  "description": "Typescript helper types",
  "author": "Vladimir Zhukov",
  "license": "MIT",
  "contributors": [
    {
      "name": "Vladimir Zhukov",
      "url": "http://github.com/vlazh"
    }
  ],
  "homepage": "https://github.com/vlazh/ts-types",
  "repository": {
    "type": "git",
    "url": "https://github.com/vlazh/ts-types"
  },
  "bugs": {
    "url": "https://github.com/vlazh/ts-types/issues"
  },
  "main": "./index.js",
  "types": "./index.d.ts",
  "scripts": {
    "update-deps": "yarn upgrade-interactive --latest",
    "clean": "rimraf dist/",
    "copy": "copyfiles package.json LICENSE README.md ./dist/",
    "build": "npm run clean && tsc --project ./tsconfig.json && npm run copy",
    "build-to-tutoron": "tsc --project ./tsconfig.json --outDir '/home/vladimir/dev/sources/tutoron/node_modules/@vzh/ts-types/'",
    "build-to-lcardio-app": "tsc --project ./tsconfig.json --outDir '/home/vladimir/dev/sources/lcardio-app/node_modules/@vzh/ts-types/'",
    "build-to-reflexy": "tsc --project ./tsconfig.json --outDir '/home/vladimir/dev/sources/reflexy/node_modules/@vzh/ts-types/'",
    "build-to-react-components": "tsc --project ./tsconfig.json --outDir '../react-components/node_modules/@vzh/ts-types/'",
    "build-to-mobx-stores": "tsc --project ./tsconfig.json --outDir '../mobx-stores/node_modules/@vzh/ts-types/'",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "patch-publish": "npm version patch && npm run build && npm publish ./dist --access public && git push --follow-tags",
    "minor-publish": "npm version minor && npm run build && npm publish ./dist --access public && git push --follow-tags",
    "major-publish": "npm version major && npm run build && npm publish ./dist --access public && git push --follow-tags"
  },
  "devDependencies": {
    "@types/jest": "^24.0.18",
    "@typescript-eslint/eslint-plugin": "^2.1.0",
    "@typescript-eslint/parser": "^2.1.0",
    "@vzh/configs": "^3.38.1",
    "copyfiles": "^2.1.1",
    "eslint": "^6.3.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.0",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "rimraf": "^3.0.0",
    "ts-jest": "^24.0.2",
    "typescript": "^3.6.2"
  }
}
